using System;

class Program
{
    static void Main()
    {
        int[] ages = new int[5];
        int teenagerCount = 0;

        for (int i = 0; i < ages.Length; i++)
        {
            while (true)
            {
                Console.Write($"Enter age for customer {i + 1} (between 10 and 99): ");
                if (int.TryParse(Console.ReadLine(), out ages[i]) && ages[i] >= 10 && ages[i] <= 99)
                {
                    if (ages[i] >= 13 && ages[i] <= 19)
                    {
                        teenagerCount++;
                    }
                    break; // Exit the loop if the input is valid
                }
                else
                {
                    Console.WriteLine("Invalid age. Please enter a value between 10 and 99.");
                }
            }
        }

        Console.WriteLine("\nValid ages entered:");
        foreach (int age in ages)
        {
            Console.WriteLine(age);
        }
        Console.WriteLine($"Number of teenagers (ages 13-19): {teenagerCount}");
    }
}
